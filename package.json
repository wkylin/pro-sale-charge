    }
  },
  "lint-staged": {
    "src/**/*.{js, jsx, ts, tsx}": [
      "npm run eslint:fix"
    ],
    "src/**/*.less": [
      "npm run stylelint:fix"
    ],
    "*.md": [
      "npm run markdownlint:fix"
    ]
  },
  "config": {
    "commitizen": {
