  "main": "dist/main.js",
  "sideEffects": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "cross-env BUILD_GOAL=development NODE_ENV=development webpack serve --config ./webpack/webpack.dev.js --stats-error-details --env dev",
    "start:sit": "cross-env BUILD_GOAL=sit NODE_ENV=development webpack serve --config ./webpack/webpack.dev.js --stats-error-details --env sit",
    "build:production": "cross-env BUILD_GOAL=production NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details --env production",
    "build:online": "cross-env BUILD_GOAL=online NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details  --env online && rimraf dist/**/*.map",
    "build:test": "cross-env BUILD_GOAL=test NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details --env test",
    "build:dev": "cross-env BUILD_GOAL=dev NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details --env dev",
    "build:sit": "cross-env BUILD_GOAL=sit NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details --env sit",
    "prod:serve": "cross-env BUILD_SERVE=prod BUILD_GOAL=production NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details --env production",
    "online:serve": "cross-env BUILD_SERVE=prod BUILD_GOAL=online NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details --env online",
    "dev:serve": "cross-env BUILD_SERVE=prod BUILD_GOAL=dev NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details --evn dev",
    "test:serve": "cross-env BUILD_SERVE=prod BUILD_GOAL=test NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details --env test",
    "sit:serve": "cross-env BUILD_SERVE=prod BUILD_GOAL=sit NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details --env sit",
    "analyze:build": "cross-env BUILD_GOAL=production NODE_ENV=production USE_ANALYZE=1 webpack --config ./webpack/webpack.prod.js --stats-error-details --env production --profile --json=compilation-stats.json",
    "serve:prod": "npm run prod:serve && serve -s dist/promotion-manage -l 5000",
    "serve:dev": "npm run dev:serve && serve -s dist/promotion-manage -l 5000",
    "serve:test": "npm run test:serve && serve -s dist/promotion-manage -l 5000",
    "serve:sit": "npm run sit:serve && serve -s dist/promotion-manage -l 5000",
    "serve:online": "npm run online:serve && serve -s dist/promotion-manage -l 5000",
    "http:dev": "npm run dev:serve && npm run http-server",
    "http:prod": "npm run prod:serve && npm run http-server",
    "http:test": "npm run test:serve && npm run http-server",
    "http:sit": "npm run sit:serve && npm run http-server",
    "http:online": "npm run online:serve && npm run http-server",
    "http-server": "http-server ./dist/promotion-manage -p 9090 --proxy http://xxx.com -o --cors --log-ip --gzip",
    "test:jest": "jest",
    "update:yarn": "yarn upgrade-interactive --latest",
    "update:npm": "ncu -u && npm install",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix && npm run prettier:fix",
    "lint": "npm run eslint && npm run stylelint && npm run prettier:check",
    "stylelint": "stylelint \"src/**/*.less\" --custom-syntax postcss-less",
    "stylelint:fix": "stylelint \"src/**/*.less\" --fix --custom-syntax postcss-less",
    "prettier:check": "prettier --check \"src/**/*\" --end-of-line auto --ignore-unknown",
    "prettier:fix": "prettier --write \"src/**/*\" --end-of-line auto --ignore-unknown",
    "prettier:fix:all": "prettier --write . --end-of-line auto --ignore-unknown",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "eslint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "markdownlint": "markdownlint *.md",
    "markdownlint:fix": "markdownlint --fix *.md",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "process": "node webpack/process"
  },
